{
    "version": 2,
    "builds": [{
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "devCommand": "npm run start"
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "headers": {
            "Access-Control-Allow-Origin": "*"
        },
        "status": 200
    }]
}